{
  "expo": {
    "name": "MymbaMobile",
    "slug": "MymbaMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo veterinaria animado azul rosado.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.agmethjimenez321.MymbaMobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a1f2f577-b094-421f-bf55-80dd888aff70"
      }
    }
  }
}
